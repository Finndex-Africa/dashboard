# Cloudinary Configuration
# These credentials are needed for direct uploads from the frontend to Cloudinary

# Cloudinary Cloud Name
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name_here

# Cloudinary Upload Preset (unsigned preset for client-side uploads)
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your_upload_preset_here

# Cloudinary API Key (optional, only needed for signed uploads)
NEXT_PUBLIC_CLOUDINARY_API_KEY=your_api_key_here

# API Base URL
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Website URL (for redirects)
NEXT_PUBLIC_WEBSITE_URL=http://localhost:3000
